    "# Step 1: Make predictions on the test set\n",
    "y_pred = model.predict([x_test[:, 0], x_test[:, 1]])\n",
    "\n",
    "# Step 2: Convert distances to binary predictions using a threshold\n",
    "# Assuming distances < 0.5 classify as genuine pair (similar) and >= 0.5 as dissimilar\n",
    "y_pred_binary = (y_pred < 0.5).astype(int)\n",
    "\n",
    "# Step 3: Flatten the y_test and y_pred_binary arrays for comparison\n",
    "y_test_binary = y_test.flatten()\n",
    "\n",
    "# Step 4: Calculate the F1 score\n",
    "from sklearn.metrics import f1_score\n",
    "f1 = f1_score(y_test_binary, y_pred_binary)\n",
    "\n",
    "# Print the F1 score\n",
    "print('F1 Score: %0.2f' % f1)"
   ]
